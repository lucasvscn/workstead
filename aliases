# .bashrc

# Source global definitions
if [ -f /etc/bashrc ]; then
        . /etc/bashrc
fi

# User specific aliases and functions

alias ..="cd .."
alias ...="cd ../.."

alias h='cd ~'
alias c='clear'

alias art='php artisan'
alias phpunit='vendor/bin/phpunit'

